!
! Asus Eee PC 1080P portuguese keyboard
!

keycode 108 = ISO_Level3_Shift
keycode 111 = Up
keycode 113 = Left
keycode 114 = Right
keycode 116 = Down
!keycode 1xx = Print Sys_Req
keycode 118 = Insert
keycode 119 = Delete
keycode 127 = Pause Break
keycode 135 = Menu

keysym Menu = Super_R
keysym ISO_Level3_Shift = Alt_R

! vim: ft=xmodmap
