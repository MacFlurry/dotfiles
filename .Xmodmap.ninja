!
! Settings for Intellimouse Explorer
!
pointer = 1 2 3 4 5 8 9 6 7 10 11

!
! Keyboard Control Keys
!
clear lock
clear shift
clear control
clear mod1
clear mod2
clear mod3
clear mod4
clear mod5

keysym Caps_Lock = Mode_switch
keysym Menu = Super_R
keysym Super_R = Mode_switch

add shift = Shift_L Shift_R
add control = Control_L Control_R
add mod1 = Alt_L Alt_R
add mod2 = Num_Lock
add mod3 = Mode_switch
add mod4 = Super_L 
add mod5 = ISO_Level3_Shift
